{
  "name": "app",
  "main": "backend/build/main.js",
  "author": "author",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "backend:build": "tsc -p backend && tsc-alias -p ./backend/tsconfig.json",
    "backend:dev": "pnpm run backend:build && electron .",
    "backend:format": "biome format ./backend/src",
    "backend:lint": "biome lint ./backend/src",
    "backend:check": "biome check ./backend/src",
    "frontend:build": "next build frontend",
    "frontend:dev": "next dev frontend",
    "frontend:format": "biome format ./frontend/src",
    "frontend:lint": "biome lint ./frontend/src",
    "frontend:check": "biome check ./frontend/src",
    "lint:write": "pnpm run backend:lw && pnpm run frontend:lw",
    "changelog": "bump changelog generate --config .bumprc.json",
    "dev": "concurrently -n \"NEXT,ELECTRON\" -c \"white,blue\" --kill-others \"pnpm run frontend:dev\" \"wait-on http://localhost:3000/ && pnpm run backend:dev\"",
    "build": "pnpm run frontend:build && pnpm run backend:build && electron-builder",
    "build:changelog": "bump changelog generate --config .bumprc.json",
    "prepare": "husky"
  },
